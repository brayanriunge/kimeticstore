generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product{
  id  String    @id @default(cuid())
  description String
  name String
  category String
  imgUrl String
  createdAt DateTime @default(now())
}
